@model WebApp.Models.Medarbejder

@{
    ViewData["Title"] = "Detaljer";
}

<h1>Detaljer</h1>

<div>
    <h4>Medarbejder</h4>
    <hr />
    <dl class="row">
        <dt class = "col-sm-2">
            Navn
        </dt>
        <dd class = "col-sm-10">
            @Html.DisplayFor(model => model.navn)
        </dd>
        <dt class = "col-sm-2">
            Telefon Nummer
        </dt>
        <dd class = "col-sm-10">
            @Html.DisplayFor(model => model.telefonNummer)
        </dd>
        <dt class = "col-sm-2">
            E-Mail
        </dt>
        <dd class = "col-sm-10">
            @Html.DisplayFor(model => model.eMail)
        </dd>
        <dt class = "col-sm-2">
            Alder
        </dt>
        <dd class = "col-sm-10">
            @Html.DisplayFor(model => model.alder)
        </dd>
    </dl>
</div>
<div>
    <a asp-action="Edit" asp-route-id="@Model?.id">Ændrer</a> |
    <a asp-action="Index">Tilbage til liste</a>
</div>
